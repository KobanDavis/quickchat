{
	"name": "quickchat",
	"license": "ISC",
	"pkg": {
		"assets": [
			"**/*.node",
			"*.node"
		]
	},
	"main": "dist/index.js",
	"scripts": {
		"package": "cd dist && pkg index.js -t node16-win-x64 --config ../package.json && cd ../",
		"transpile": "rimraf dist && tsc",
		"build": "cd dist && ncc build index.js -o . && yarn package && cd ../",
		"dev": "nodemon src/index.ts"
	},
	"dependencies": {
		"@nut-tree/nut-js": "^3.1.1",
		"@uspasojevic/dualshock4": "^1.0.0",
		"dualsense-ts": "^5.1.8",
		"node-hid": "^2.1.2"
	},
	"devDependencies": {
		"@vercel/ncc": "^0.36.1",
		"nodemon": "^2.0.21",
		"pkg": "^5.8.1",
		"rimraf": "^4.4.0",
		"ts-node": "^10.9.1",
		"typescript": "^4.9.5"
	},
	"version": "0.4.0"
}